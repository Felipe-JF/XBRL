{
  "tasks": {
    "start": "deno run src/main.ts",
    "test": "deno test",
    "dev:csv": "deno run --allow-read=./static src/devCSV.ts "
  },
  "lint": {
    "rules": {
      "exclude": ["require-await"]
    }
  },
  "fmt": {
    "options": {
      "indentWidth": 2
    }
  },
  "compilerOptions": {
    "noUncheckedIndexedAccess": true
  },
  "importMap": "import_map.json"
}
